  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.6",
    "@iconify-json/carbon": "^1.1.17",
    "@types/node": "^18.16.16",
    "@unocss/eslint-config": "^0.51.13",
    "@unocss/reset": "^0.51.13",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue-macros/volar": "^0.9.8",
    "@vue/test-utils": "^2.3.2",
    "eslint": "^8.42.0",
